<div class="col">
  <div class="card" style="width: 18rem">
    <img src=<%= item.thumbnail %> class="card-img-top" alt=<%=item.title%> />
    <div class="card-body">
      <h5 class="card-title"> <%= item.title%> </h5>
      <h6 class="card-subtitle mb-2 text-muted">Precio: <%= item.price%></h6>
    </div>
  </div>
</div>
