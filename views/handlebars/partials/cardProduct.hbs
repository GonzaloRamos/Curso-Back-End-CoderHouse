<div class="col">

  <div class="card" style="width: 18rem;">
    <img src="{{this.thumbnail}}" class="card-img-top" alt="{{this.title}}" />
    <div class="card-body">
      <h5 class="card-title">{{this.title}}</h5>
      <h6 class="card-subtitle mb-2 text-muted">Precio: ${{this.price}}</h6>

    </div>
  </div>
</div>